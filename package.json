{
	"name": "vscode-cdp-proxy",
	"description": "Sample Chrome Debug Protocol proxy used to extend vscode-js-debug",
	"main": "dist/index.js",
	"scripts": {
		"Document": "Document 'Source/**/*.ts'",
		"compile": "rimraf dist && tsc && cpy src/cdp.d.ts dist",
		"prepublishOnly": "Build 'Source/**/*.ts'",
		"test:unit": "mocha --opts mocha.opts"
	},
	"dependencies": {
		"cockatiel": "3.1.3",
		"ws": "8.17.0"
	},
	"devDependencies": {
		"@playform/build": "0.0.7",
		"@types/chai": "4.3.16",
		"@types/node": "20.12.10",
		"@types/ws": "8.5.10",
		"chai": "5.1.0",
		"cpy-cli": "5.0.0",
		"devtools-protocol": "0.0.1295939",
		"rimraf": "5.0.5"
	}
}
